<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_F7"
    tools:context=".workbench.invoice.activity.InvoiceDetailsActivity">

    <com.yhhl.yhyq.util.StatusBarHeightView
        android:id="@+id/start"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:use_type="use_padding_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />

    <include layout="@layout/title_layout"
        app:layout_constraintTop_toBottomOf="@+id/start"
        android:id="@+id/title_in"/>

    <LinearLayout
        android:id="@+id/linear1"
        app:layout_constraintTop_toBottomOf="@+id/title_in"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:text="公  司  名  称 ："
            android:layout_marginLeft="@dimen/dp_15"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/company_name"
            android:text="北京市云合互联科技有限公司"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/dp_1"
        android:id="@+id/linear2"
        app:layout_constraintTop_toBottomOf="@+id/linear1"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:text="纳税人识别号："
            android:layout_marginLeft="@dimen/dp_15"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/ITIN"
            android:text="91110114335445191K"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/dp_1"
        android:id="@+id/linear3"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear2"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="地             址："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/address"
            android:text="北京市昌平区回龙观大姐龙冠上午中心"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear4"
        android:layout_marginTop="@dimen/dp_1"
        app:layout_constraintTop_toBottomOf="@+id/linear3"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="电             话："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/phone"
            android:text="17600077655"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear5"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear4"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_marginBottom="@dimen/dp_16"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="开     户     行："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/opening_bank"
            android:text="招商银行股份有限公司北京大运村支行"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear6"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear5"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_marginBottom="@dimen/dp_16"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="银  行  账  户："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/bank_account"
            android:text="110915285410701"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear7"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear6"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_marginBottom="@dimen/dp_16"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="发  票  内  容："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/invoice_content"
            android:text="日常常规通讯报销"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear8"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear7"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_marginBottom="@dimen/dp_16"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="税             率："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tax_rate"
            android:text="0.5"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear9"
        android:gravity="center_vertical"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/linear8"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_marginBottom="@dimen/dp_16"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:text="发  票  金  额："
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/invoice_value"
            android:text="0.5"
            android:textSize="@dimen/textSize15"
            android:textColor="@color/color_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <Button
        android:id="@+id/back_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_22"
        android:text="返回"
        android:layout_marginBottom="@dimen/dp_64"
        android:background="@drawable/shape_login_btn"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>